{
  "name": "ttrack-server",
  "version": "0.6.0",
  "description": "Another TimeTracking Tool",
  "author": "25th floor",
  "license": "MIT",
  "main": "src/server.js",
  "scripts": {
    "migration": "db-migrate up",
    "migration-test": "db-migrate up --config database.json -e test",
    "test": "NODE_ENV=test jest --bail --no-cache",
    "start": "npm-run-all --parallel lint start:server",
    "start:server": "nodemon --exec babel-node src/server.js",
    "start:production": "babel-node src/server.js",
    "lint": "eslint . --ext .js"
  },
  "private": false,
  "ttrackServer": {
    "apiVersion": 1,
    "validVersions": [
      1
    ]
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "blipp": "^3.0.0",
    "db-migrate": "^0.11.1",
    "db-migrate-pg": "^0.4.0",
    "glue": "^5.0.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^17.5.3",
    "hapi-405-routes": "^0.7.1",
    "hapi-api-version": "^2.0.0",
    "hapi-swagger": "^9.1.1",
    "inert": "^5.1.0",
    "joi-date-extensions": "^1.0.2",
    "knex": "^0.15.2",
    "moment": "^2.18.1",
    "moment-duration-format": "^2.2.2",
    "npm-run-all": "^4.0.2",
    "pg": "^7.0.2",
    "q": "^1.5.0",
    "ramda": "^0.25.0",
    "sql": "^0.78.0",
    "sql-formatter": "^2.0.0",
    "twix": "^1.2.1",
    "vision": "^5.3.3"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "~6.24",
    "babel-preset-stage-2": "^6.24.1",
    "chakram": "^1.5.0",
    "code": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jest": "^21.18.0",
    "istanbul": "^0.4.5",
    "jasmine": "~3.1",
    "jasmine-check": "git://github.com/dready/jasmine-check.git#master",
    "jasmine-expect": "~3.8",
    "jasmine-expect-moment": "~0.1",
    "jasmine-reporters": "~2.3",
    "jest-cli": "~23.4",
    "lab": "^15.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "globalSetup": "./globalSetup.js",
    "globalTeardown": "./globalTeardown.js",
    "collectCoverage": true,
    "testEnvironment": "node",
    "roots": [
      "./src/api"
    ]
  },
  "engines": {
    "node": ">=8.10.0"
  }
}
